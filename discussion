// test code
@function test(a : Integer, b : Integer) : Integer {
    a + b
}
@function test2(a : Integer) : Integer {
    2 * a
}

// ast for this showed that it is regarded as a "call", due to this in fwiki.pegjs
postfix_expression
    = head:template_member_expression tail:template_member_expression* {
        function recursion(head, tail) {
            if (tail.length === 0) {
                return head;
            } else {
                  return {
                    node : "call", // this line
                    function : recursion(head, tail.slice(0, tail.length - 1)), 
                    argument : tail[tail.length - 1]
                };
            }
        }
        return recursion(head, tail);
    }
// should this be regarded as a "call" in this case?