@var<T> if(cond : Bool, then : T, else : T) : T = @native(if : T)

@class Integer {
    @var @operator+(rhs : Integer) : Integer = @native(plus : Integer);
    @var @operator-(rhs : Integer) : Integer = @native(minus : Integer);
    @var @operator*(rhs : Integer) : Integer = @native(times : Integer);
}
